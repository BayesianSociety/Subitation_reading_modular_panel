      "0": [
        {
          "text": "Mooi,' zegt buurman",
          "tags": [
            "motivatie"
          ]
        },
        {
          "text": "Jip en Janneke gaan op hun knietjes liggen",
          "tags": [
            "vreugde"
          ]
        },
        {
          "text": "Sneeuwklokjes luiden nooit",
          "tags": [
            "aandacht"
          ]
        },
        {
          "text": "Maar o jee",
          "tags": [
            "vriendelijkheid"
          ]
        },
        {
          "text": "Te laat,' zegt Janneke boos",
          "tags": [
            "samen"
          ]
        },
        {
          "text": "En dan gaat ze naar Jip z'n huis",
          "tags": [
            "creativiteit"
          ]
        },
        {
          "text": "Nee hoor",
          "tags": [
            "mindfulness"
          ]
        },
        {
          "text": "Dag kinderen,' zegt hij",
          "tags": [
            "zelfvertrouwen"
          ]
        },
        {
          "text": "Je moet het's morgens vroeg proberen, zegt buurman",
          "tags": [
            "groei"
          ]
        },
        {
          "text": "Maar ze horen niets",
          "tags": [
            "leren"
          ]
        },
        {
          "text": "De oude buurman kijkt over de heg",
          "tags": [
            "zorg"
          ]
        },
        {
          "text": "Hij is al half aangekleed",
          "tags": [
            "rust"
          ]
        },
        {
          "text": "Heel vroeg",
          "tags": [
            "vriendelijkheid"
          ]
        },
        {
          "text": "die net thee zet",
          "tags": [
            "samenwerking"
          ]
        },
        {
          "text": "Dan luiden de sneeuwklokjes",
          "tags": [
            "inspiratie"
          ]
        },
        {
          "text": "Daar komt Jip",
          "tags": [
            "blijheid"
          ]
        },
        {
          "text": "daar zijn de sneeuwklokjes",
          "tags": [
            "nieuwsgierigheid"
          ]
        },
        {
          "text": "blijf maar lekker in je bed",
          "tags": [
            "begrip"
          ]
        },
        {
          "text": "Dan moet je goed luisteren",
          "tags": [
            "mindset"
          ]
        },
        {
          "text": "Ja,' zegt Jip verdrietig",
          "tags": [
            "rust",
            "beeldspraak"
          ]
        }
